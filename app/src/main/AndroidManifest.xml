<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.osw.quizmaker">

    <uses-permission android:name="android.permission.INTERNET" />

    <supports-screens android:requiresSmallestWidthDp="600" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateMPActivity" />
        <activity android:name=".CreateTFActivity" />
        <activity android:name=".CreateNPActivity" />
        <activity android:name=".CreateFromXmlActivity" />
        <activity android:name=".QuestionListActivity" />
        <activity android:name=".ViewInfoActivity" />
        <activity android:name=".QuestionDetails"></activity>
    </application>

</manifest>